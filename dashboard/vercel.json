{
  "version": 2,
  "name": "krayons-link-dashboard",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/events/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/auth(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/((?!.*\\.).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "browsing-topics=(), run-ad-auction=(), join-ad-interest-group=(), private-aggregation=(), attribution-reporting=(), compute-pressure=()"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}